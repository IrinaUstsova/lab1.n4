#include <iostream>
#include <cmath>

int main() {
  
  std::cout << "Hello World!\n";
  unsigned short int c;
  unsigned short int d;
  unsigned short int e;
  unsigned short int f;
  unsigned short int a;
  std::cout << "Enter c:";
  std::cin >>c;
  std::cout << "Enter d:";
  std::cin >>d;
  std::cout << "Enter e:";
  std::cin >>e;
  std::cout << "Enter f:";
  std::cin >>f;
  a=(!(c+d))*(e*!f+!e*f);
  std::cout<<a<<std::endl;
  std::cout<<std::hex<<a<<std::endl;
return a;
}
